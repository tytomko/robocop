    </xacro:ultraSound>

    <xacro:ultraSound name="face">
        <origin rpy="0 0 0" xyz="0.2747 0.0 -0.0088"/>
    </xacro:ultraSound> -->
  <!-- <xacro:lidar parent="trunk">
        <origin rpy="0 0 0" xyz="0.05 0 0.08"/>
    </xacro:lidar> -->
  <joint name="laser_joint" type="fixed">
    <parent link="trunk"/>
    <child link="laser_link"/>
    <origin rpy="0 0 0" xyz="0.05 0.0 0.1"/>
  </joint>
  <link name="laser_link">
    <collision>
      <origin rpy="0 0 0" xyz="0.05 0 0.5"/>
      <geometry>
        <cylinder length="0.05" radius="0.05"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.07" radius="0.05"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <mass value="1e-5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6"/>
    </inertial>
  </link>
  <gazebo reference="laser_link">
    <sensor name="lidar" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>450</samples>
            <resolution>1</resolution>
            <min_angle>0.0</min_angle>
            <max_angle>6.28319</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.4</min>
          <max>35.0</max>
          <resolution>0.015</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_ray_sensor.so" name="lidar_controller">
        <ros>
          <remapping>~/out:=scan</remapping>
        </ros>
        <output_type>sensor_msgs/LaserScan</output_type>
        <frame_name>laser_link</frame_name>
      </plugin>
    </sensor>
  </gazebo>
  <gazebo>
    <plugin filename="libgazebo_ros_p3d.so" name="p3d_ground_truth">
      <!-- <robotNamespace>${g_robot_name}</robotNamespace> -->
      <body_name>base_link</body_name>
      <topic_name>odom</topic_name>
      <!-- <frame_name>world</frame_name> -->
      <update_rate>100</update_rate>
    </plugin>
  </gazebo>
  <joint name="gps_joint" type="fixed">
    <parent link="trunk"/>
    <child link="gps_link"/>
    <!-- gps 센서를 trunk 상단에 배치 (예: z 방향 0.2m 위) -->
    <origin rpy="0 0 0" xyz="0 0 0.2"/>
  </joint>
  <link name="gps_link">
    <!-- 단순한 시각화용 박스 (크기 0.01m 정도) -->
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <inertial>
      <mass value="0.001"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0001" ixy="0" ixz="0" iyy="0.0001" iyz="0" izz="0.0001"/>
    </inertial>
  </link>
</robot>
