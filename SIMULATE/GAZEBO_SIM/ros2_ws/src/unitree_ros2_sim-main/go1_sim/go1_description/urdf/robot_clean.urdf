  <gazebo reference="lidar">
    <sensor name="lidar" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <visualize>false</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>450</samples>
            <resolution>1</resolution>
            <min_angle>0.0</min_angle>
            <max_angle>6.28319</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.120</min>
          <max>35.0</max>
          <resolution>0.015</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin filename="libgazebo_ros_ray_sensor.so" name="lidar_controller">
        <ros>
          <remapping>~/out:=scan</remapping>
        </ros>
        <output_type>sensor_msgs/LaserScan</output_type>
        <frame_name>lidar</frame_name>
      </plugin>
    </sensor>
  </gazebo>
  <gazebo reference="gps_link">
    <gravity>true</gravity>
    <sensor name="ssafy_gps" type="gps">
      <always_on>true</always_on>
      <!-- 원하는 업데이트 주파수 (예: 10 Hz) -->
      <update_rate>10</update_rate>
      <!-- 필요에 따라 시각화 여부 (false 권장) -->
      <visualize>false</visualize>
      <!-- 전역 토픽 이름으로 publish -->
      <topic>/ssafy_gps</topic>
      <!-- SDF 규격에 맞는 노이즈 설정 (노이즈 없으면 0.0) -->
      <noise type="gaussian">
        <mean>0.0</mean>
        <stddev>0.0</stddev>
      </noise>
      <plugin filename="libgazebo_ros_gps_sensor.so" name="ssafy_gps_plugin">
        <!-- 플러그인 내부 설정: publish 토픽, 기준 프레임 등 -->
        <topicName>/ssafy_gps</topicName>
        <bodyName>gps_link</bodyName>
        <updateRateHZ>10.0</updateRateHZ>
        <!-- 필요에 따라 추가 파라미터들을 넣을 수 있습니다. -->
      </plugin>
      <!-- 센서의 설치 위치 (필요에 따라 수정) -->
      <pose>0 0 0 0 0 0</pose>
    </sensor>
  </gazebo>
